{
#	Dataset in the realtime data base
	"dataSet": "DataSet1",
	"prefix": {
		"publish": "", 
		"subscribe": "", 
		"internal": ""
	},
#	Connection attributes to Mosquitto brocker
	"connection": {
		"broker": "tcp://localhost:1883",
		"credentials": {
			"username": "",
			"password": "",
			"clientID": "ID"
		},
#	Quality of service for the MQTT service, for both Pub&Sub
		"qos": {
			"publish": 1,
			"subscribe": 1
		}
	},
#	The periodicity for transmitting publishable data & reconnect
	"period": 2000,
#	Files for list of publishable data
	"pubAssociation": {
		"LD01-Partial": {
# 				another alias:
			"regexSelection": "Sys.*",
			"mqttTopic": "System",
			"StartTime": "2018-02-17 03:06:00",
			"EndTime": "",
# Here we have some scaling rules for voltage		
			"rescaleRules": [ [ "SysMemoryLoad", 0.01] ],
			"renameRules": [ [ "SysMemoryLoad", "SML"] ],
			"addItemRules": [ 
				[ "SMLU", "%" ], 
				[ "SMXtimestamp", "!Timestamp{Custom yyyy/mm/dd hh:MM:ss:SSS}{UTC}" ] 
			]
		}
	},
#	Files for list of data which is requested from Mosqutto
#	for subscribtion (these data are expected to be received
	"subAssociation": {
		"DRCC1ctrl": {
# 				another alias:
			"internalName": "SMX/DRCC1.DERStr.ctlNum",
			"mqttTopic": "/ER/SMX/EnergyRouterInverter/DRCC1.DERStr.ctlNum",
			"extractFirst": true,
			"readJson": true,
			"StartTime": "2018-02-17 03:06:00",
			"EndTime": ""
		},
		"ZBTC1batchasetmag": {
# 				another alias:
			"internalName": "SMX/ZBTC1.BatChaPwr.setMag.f",
			"mqttTopic": "/ER/SMX/EnergyRouterInverter/ZBTC1.BatChaPwr.setMag.f",
			"extractFirst": true,
			"readJson": true,
			"StartTime": "2018-02-17 03:06:00",
			"EndTime": ""
		}
	}
#/ER/SMX/EnergyRouterInverter/ZBTC1.BatChaPwr.setMag.f
#
}
